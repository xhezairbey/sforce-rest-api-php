{
    "name": "xhezairi/sforce-rest-api-php",
    "description": "A Salesforce REST API wrapper for PHP applications.",
    "keywords": ["salesforce", "rest", "api", "wrapper", "php", "library"],
    "homepage": "https://github.com/xhezairi/sforce-rest-api-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arian Xhezairi",
            "email": "arian@xhezairi.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.0",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/http-factory": "^1.0.1",
        "psr/http-client": "^1.0.1",
        "guzzlehttp/guzzle": "^7.1@dev",
        "monolog/monolog": "^2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5||^9.0",
        "symfony/var-dumper": "^5.2@dev",
        "phpstan/phpstan": "^0.12.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Xhezairi\\SForce\\": "src/"
        }
    }
}
